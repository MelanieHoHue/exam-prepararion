<div class="row">
<h2 class="center">Users</h2>
<table class="table-responsive">
    <tbody>
    <% users.forEach(user => { %>
    <tr>
        <td>
            <a href="<%=`/users/${user._id}`%>">
                <%= user.name %>
            </a>
        </td>
        <td>
            <%= user.age %>
        </td>
        <td>
            <a href="<%=`/users/${user._id}/edit` %>" >Edit</a>
        </td>
        <td>
            <a href="<%=`/users/${user._id}/delete?_method=DELETE` %>" >Delete</a>
        </td>
    </tr>
    <% }); %>
    </tbody>
</table>
</div>
